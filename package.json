  "repository": {
    "type": "git",
    "url": "git@github.com:unimonkiez/webpack-boilerplate.git"
  },
  "keywords": [
    "npm",
    "install"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unimonkiez/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/unimonkiez/webpack-boilerplate",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^12.0.0",
