{
  "name": "webpack-boilerplate",
  "version": "0.0.1",
  "description": "webpack-boilerplate",
  "scripts": {
    "lint": "eslint --ext .js,.jsx .",
    "test": "npm run lint",
    "build": "node ./build.js",
    "build:watch": "npm run build -- -w",
